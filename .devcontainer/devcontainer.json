{
  "name": "AlphaLensAI DevContainer",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "features": {
    "ghcr.io/devcontainers/features/copilot-cli:1": {}
  },
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/usr/local/bin/python3.11"
      },
      "extensions": [
        "GitHub.copilot",
        "ms-python.python"
      ]
    }
  }
}
